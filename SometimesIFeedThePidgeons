//03.12.20?
#include <iostream>
#include <windows.h>
#include <stdio.h>
using namespace std;

int main() {
	SetConsoleCP(1251);
	SetConsoleOutputCP(1251);
	setlocale(0, "Russian");
	int M[5][5];
	cout << "Введите числа в массив: ";
	for (int i = 0; i < 5; i++)
		for (int j = 0; j < 5; j++)
			cin >> M[i][j];
	for (int i = 0; i < 5; i++) {
		for (int j = 0; j < 5; j++)
			cout << M[i][j] << ' ';
		cout << endl;
	}
	cout << endl;
	cout << "Максимальные элементы строк: ";
	for (int i = 0; i < 5; i++) {
		int max = M[i][0];
		for (int j = 0; j < 5; j++)
			if (M[i][j] > max) max = M[i][j];
		cout << max << ' ';
	}
	cout << endl;
	cout << "Минимальные элементы столбцов: ";
	for (int j = 0; j < 5; j++) {
		int min = M[0][j];
		for (int i = 0; i < 5; i++)
			if (M[i][j] < min) min = M[i][j];
		cout << min << ' ';
	}
}
